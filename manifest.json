{
    "manifest_version": 2,
    "name": "InstaBrowser",
    "version": "1.0",
  
    "description": "Browses Instagram",
  
    "icons": {
      "48": "icons/border-48.png"
    },

    "permissions": ["activeTab", "contextMenus"],


      "background": {
        "scripts": ["instabrowser.js"]
      },

      "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["other_background.js"]
        }
      ]

  }
  